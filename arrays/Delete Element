#include <iostream>
using namespace std;


int deleteelement(int a[], int n, int x)
{

	for (int i=0; i<n-1; i++)

	if(x==a[i]){
		a[i ] = a[i+1];
	}
    return n-1;

}

int main()

{
    int a[100];
     int n;
     cout<<"enter sie of aray"<<endl;
    cin>>n;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    int x;

     cout<<"enter what to delete"<<endl;
     cin>>x;
   n= deleteelement(a,n,x);

     for( int i=0;i<n;i++)
     {
         cout<<a[i]<<" ";
     }
      return 0;

}
