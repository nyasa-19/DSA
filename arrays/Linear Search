#include<iostream>
using namespace std;


 int  linersearch(int a[],int n,int key)
{
    for(int i=0;i<n;i++)
    {
        if(a[i]==key)
            return i;
    }
    return -1;

    }

 int main()
 {
     int n;
     cin>>n;
     int a[100];
     for(int i=0;i<n;i++)
     {
         cin>>a[i];
     }
      int low=0, high=0,key ;
       cin>>key;
   cout<<linersearch(a,n,key);

 }
