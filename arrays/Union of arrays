#include<iostream>

 using namespace std;


   void unionarray(int a[],int m,int b[],int n)
   {
   int c[25];
   int i=0,j=0,k=0;

         for(i=0;i<25;i++)
         {
            cout<<c[i]<<" ";

         }



    while(i<m&&j<n)
         {
             if(a[i]=b[j])
                {
                    c[k]=a[i];
                    i++;
                    k++;
                }
                else if(b[j]<a[i])
                {
                c[k]=b[j];
                k++;
                j++;

                }
                 else{
                 c[k]=a[i];
                 i++;j++;k++;
                 }
         }

         while(i<m)
         {
             c[k]=a[i];
             i++;
             k++;

         }

         while(j<n)
         {
             c[k]=b[j];
             j++;
             k++;
         }
         for(i=0;i<25;i++)
         {
            cout<<c[i]<<endl;

         }
}



  int main()
  {

    int i=0,j=0;
   int a[10]={1,3,5,4,7,8,3,6,5,3,};
   int b[12]={4,2,5,9,9,6,3,5,7,4,7,2};

unionarray(a,10,b,12);
return 0;
}
