#include<iostream>
using namespace std;


 int  bsearch(int a[],int low, int high,int key)
{
    while(low<=high)
    {
        int mid=(low+high)/2;
        if(a[mid]==key)
            return mid;
        else if(a[mid]>key)
            high=mid-1;
        else
            low=mid+1;


    }
    return -1;
}
 int main()
 {
     int n;
     cin>>n;
     int a[100];
     for(int i=0;i<n;i++)
     {
         cin>>a[i];
     }
      int low=0, high=0,key ;
       cin>>key;
   cout<<bsearch(a,low,high,key);

 }
